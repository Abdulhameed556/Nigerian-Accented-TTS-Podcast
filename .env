# Environment variables for YarnGPT TTS API

# Model paths and configuration
TOKENIZER_PATH=saheedniyi/YarnGPT
WAV_TOKENIZER_CONFIG_PATH=wavtokenizer_mediumdata_frame75_3s_nq1_code4096_dim512_kmeans200_attn.yaml
WAV_TOKENIZER_MODEL_PATH=wavtokenizer_large_speech_320_24k.ckpt

# Model download URLs
MODEL_CONFIG_URL=https://huggingface.co/novateur/WavTokenizer-medium-speech-75token/resolve/main/wavtokenizer_mediumdata_frame75_3s_nq1_code4096_dim512_kmeans200_attn.yaml
MODEL_CHECKPOINT_URL=https://drive.google.com/uc?id=1-ASeEkrn4HY49yZWHTASgfGFNXdVnLTt

# Server configuration
PORT=8000
HOST=0.0.0.0
DEBUG=false

# Audio settings
SAMPLE_RATE=24000
CHUNK_WORD_LIMIT=25
MAX_TEXT_LENGTH=1000

# TTS Generation settings
TEMPERATURE=0.1
REPETITION_PENALTY=1.1
MAX_LENGTH=4000

# File paths
TEMP_DIR=/tmp
LOG_LEVEL=INFO
TORCH_HOME=/tmp/torch_cache

# Dependencies versions (for reference)
# outetts==0.2.3
# uroman
# transformers
# torch
# torchaudio
# gdown